{
  "name": "mini-shiki",
  "version": "3.4.2",
  "type": "module",
  "repository": "git+https://github.com/un-ts/mini-shiki.git",
  "homepage": "https://github.com/un-ts/mini-shiki#readme",
  "license": "MIT",
  "packageManager": "yarn@4.9.1",
  "exports": {
    ".": {
      "types": "./dist/shiki.d.ts",
      "default": "./dist/shiki.js"
    },
    "./onig.wasm": {
      "import": "./dist/onig.wasm"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "static"
  ],
  "scripts": {
    "build": "tsdown && cp node_modules/shiki/dist/onig.wasm dist/onig.wasm",
    "format": "prettier --write .",
    "lint": "yarn build && eslint",
    "prepare": "simple-git-hooks && yarn-berry-deduplicate || exit 0",
    "release": "yarn build && clean-pkg-json && changeset publish",
    "test": "node --experimental-require-module --test",
    "version": "changeset version && yarn --no-immutable"
  },
  "dependencies": {
    "@shikijs/engine-oniguruma": "^3.4.2",
    "@shikijs/langs": "^3.4.2",
    "@shikijs/themes": "^3.4.2",
    "@shikijs/types": "^3.4.2",
    "@shikijs/vscode-textmate": "^10.0.2"
  },
  "devDependencies": {
    "@1stg/common-config": "^14.0.0",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.4",
    "@commitlint/cli": "^19.8.1",
    "@types/node": "^22.9.3",
    "clean-pkg-json": "^1.3.0",
    "eslint": "^9.27.0",
    "nano-staged": "^0.8.0",
    "prettier": "^3.5.3",
    "semver": "7.6.3",
    "shiki": "^3.4.2",
    "simple-git-hooks": "^2.13.0",
    "tsdown": "^0.12.1",
    "yarn-berry-deduplicate": "^6.1.3"
  }
}
